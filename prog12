import turtle
turtle = turtle.Turtle()
turtle.position()

t.home()
t.clear()

for x in range(10):
  t.forward(200)
  t.left(70)

turtle.home()
turtle.clear()
turtle.forward(200)
turtle.left(120)
turtle.forward(200)
turtle.left(120)
turtle.forward(200)


turtle.home()
turtle.clear()
for i in range(3):
  turtle.forward(200)
  turtle.left(120)

turtle.home()
turtle.clear()
for i in range(5):
  turtle.forward(200)
  turtle.left(144)

t.forward(100)
t.up()
t.forward(100)
t.down()
t.forward(100)


